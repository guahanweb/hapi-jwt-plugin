{
    "rules": {
        "brace-style": 2,
        "comma-style": 2,
        "consistent-this": 2,
        "eol-last": 2,
        "key-spacing": 2,
        "new-cap": 2,
        "no-array-constructor": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": 2,
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-spaced-func": 2,
        "semi": [2, "always"],
        "semi-spacing": 2,
        "space-after-keywords": 2,
        "space-before-blocks": 2,
        "space-infix-ops": 2,
        "space-return-throw-case": 2,
        "space-unary-ops": 2,
        "spaced-comment": 2,
        "quotes": [1, "single"],
        "es6": true
    }
}
